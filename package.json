{
  "name": "portrait-dialog-editor",
  "productName": "PortraitDialogEditor",
  "version": "0.3.0",
  "description": "Editor for portraitone and friends",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "hot-server": "node server.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "start": "cross-env NODE_ENV=production electron ./",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "package": "cross-env NODE_ENV=production node package.js",
    "package-darwin": "npm run package -- --darwin",
    "package-all": "npm run package -- --all"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lostfictions/portrait-dialog-editor.git"
  },
  "keywords": [],
  "author": "s",
  "license": "GPL-3.0",
  "bugs": {
    "url": ""
  },
  "homepage": "https://github.com/lostfictions/portrait-dialog-editor",
  "devDependencies": {
    "asar": "^0.9.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.37.2",
    "electron-rebuild": "^1.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "react-addons-perf": "^0.14.7",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-logger": "^2.3.1",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "bootswatch": "^3.3.6",
    "electron-debug": "^0.5.2",
    "font-awesome": "^4.5.0",
    "fs-extra": "^0.26.5",
    "immutable": "^3.7.6",
    "react": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "react-draggable": "^1.3.4",
    "react-list": "^0.7.15",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "reselect": "^2.1.0",
    "velocity-react": "^1.1.4"
  }
